<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.VentaDetalleMapper">

    <insert id="insertVentaDetalles" parameterType="modelos.Venta" useGeneratedKeys="true"
            keyProperty="id_venta_detalle">
        insert into ventadetalle (cantidad, id_venta, id_producto) values
        <foreach item="item" collection="ventaDetalles" separator=",">
            (#{item.cantidad}, #{item.id_venta}, #{item.id_producto})
        </foreach>
    </insert>



</mapper>